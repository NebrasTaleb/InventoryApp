<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nebras.inventoryappstagetwo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.nebras.inventoryappstagetwo.MainActivity"
            android:label="@string/main_activity_title">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.nebras.inventoryappstagetwo.EditorActivity">

        </activity>
        <activity
            android:name="com.example.nebras.inventoryappstagetwo.DetailsActivity"
            android:label="@string/product_details">

        </activity>

        <provider
            android:name=".SQLite.ProductProvider"
            android:authorities="com.example.nebras.inventoryappstagetwo"
            android:exported="false" />
    </application>

</manifest>